<%- include("../partials/classes-header.ejs"); -%>
<form action="/submitAssn/<%=id%>" method="post" enctype='multipart/form-data'>
  <div class="form-group">
    <label for="exampleInputEmail1">Full Name</label>
    <input type="text" name="name" class="form-control border border-info" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Roll no.</label>
    <input type="number" name="rollno" class="form-control border border-info" id="exampleInputPassword1" >
  </div>
  <div class="mb-3">
  <label for="formFile" class="form-label border border-info">Upload a file</label>
  <input class="form-control" name="ass-sub" type="file" id="formFile">
</div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<%- include("../partials/classes-footer.ejs"); -%>